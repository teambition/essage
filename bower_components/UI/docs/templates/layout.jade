!!!
html
  head
    meta(charset="utf-8")
    title #{title}
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(href="assets/css/teambition-ui.css", rel="stylesheet")
    link(href="assets/css/teambition-ui-responsive.css", rel="stylesheet")
    link(href="assets/css/docs.css", rel="stylesheet")
    link(href="assets/js/emojify/emojify.css",rel="stylesheet")
    link(href="assets/js/google-code-prettify/prettify.css",rel="stylesheet")
    link(rel="apple-touch-icon-precomposed", sizes="144x144", href="assets/ico/apple-touch-icon-144-precomposed.png")
    link(rel="apple-touch-icon-precomposed", sizes="114x114", href="assets/ico/apple-touch-icon-114-precomposed.png")
    link(rel="apple-touch-icon-precomposed", sizes="72x72", href="assets/ico/apple-touch-icon-72-precomposed.png")
    link(rel="apple-touch-icon-precomposed", href="assets/ico/apple-touch-icon-57-precomposed.png")
    link(rel="shortcut icon", href="assets/ico/favicon.png")

    //- [if lt IE 9]
    //- <script src="assets/js/html5shiv.js"></script>
    //- ![endif]

  body.vertical-scroll(data-spy="scroll", data-target=".bs-docs-sidebar")
    div.navbar.navbar-fixed-top
      div.navbar-inner
        div.container
          button.btn.btn-navbar(type="button", data-toggle="collapse", data-target=".nav-collapse")
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.brand(href="./index.html") teambition UI
          div.nav-collapse.collapse
            ul.nav.pull-right
              li.index
                a(href="./index.html") Home
              li.getting-started
                a(href="./getting-started.html") Get started
              li.base-css
                a(href="./base-css.html") Base CSS
              li.components
                a(href="./components.html") Components
              li.scaffolding
                a(href="./scaffolding.html") Scaffolding
              li.javascript
                a(href="./javascript.html") JavaScript

    block content

    footer.footer
      div.container
        p by teambition
        p
          span with special thanks to bootstrap team

    script(src="assets/js/jquery.js")
    script(src="assets/js/bootstrap-transition.js")
    script(src="assets/js/bootstrap-alert.js")
    script(src="assets/js/bootstrap-modal.js")
    script(src="assets/js/bootstrap-dropdown.js")
    script(src="assets/js/bootstrap-accordion.js")
    script(src="assets/js/bootstrap-scrollspy.js")
    script(src="assets/js/bootstrap-tab.js")
    script(src="assets/js/bootstrap-tooltip.js")
    script(src="assets/js/bootstrap-popover.js")
    script(src="assets/js/bootstrap-datepicker.js")
    script(src="assets/js/bootstrap-button.js")
    script(src="assets/js/bootstrap-collapse.js")
    script(src="assets/js/bootstrap-carousel.js")
    script(src="assets/js/bootstrap-typeahead.js")
    script(src="assets/js/bootstrap-affix.js")
    script(src="assets/js/holder/holder.js")
    script(src="assets/js/google-code-prettify/prettify.js")
    script(src="assets/js/emojify/emojify.min.js")
    script(src="assets/js/application.js")
    script
      emojify.setConfig({
            emoticons_enabled: true,
            people_enabled: true,
            nature_enabled: true,
            objects_enabled: true,
            places_enabled: true,
            symbols_enabled: true
      });
      emojify.run();
    block script